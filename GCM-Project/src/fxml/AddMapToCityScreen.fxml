<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="534.0"
	prefWidth="644.0" xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ComboBox fx:id="mapsCombo" layoutX="113.0" layoutY="117.0"
			prefHeight="25.0" prefWidth="373.0" promptText="CHOOSE MAP" />
		<Label layoutX="109.0" layoutY="51.0" prefHeight="65.0"
			prefWidth="382.0" text="ADD MAP TO CITY" textFill="#2803fa">
			<font>
				<Font size="44.0" />
			</font>
		</Label>
		<ImageView fx:id="mapImage" fitHeight="262.0"
			fitWidth="382.0" layoutX="14.0" layoutY="150.0" pickOnBounds="true"
			preserveRatio="true" />
		<Button fx:id="AddButton" layoutX="368.0" layoutY="472.0"
			mnemonicParsing="false" onMouseClicked="#AddBtnClick"
			prefHeight="25.0" prefWidth="118.0" text="Add" />
		<Button fx:id="CancelButton" layoutX="504.0" layoutY="472.0"
			mnemonicParsing="false" onMouseClicked="#CancelBtnClick"
			prefHeight="25.0" prefWidth="125.0" text="Cancel" />
		<TextArea fx:id="infoText" layoutX="416.0" layoutY="181.0"
			prefHeight="200.0" prefWidth="221.0" promptText="INFORMATION" />
		<Label fx:id="cityLbl" layoutY="8.0" prefHeight="51.0"
			prefWidth="162.0" text="Label">
			<font>
				<Font size="35.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
