<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-1.0" maxWidth="-1.0"
	prefHeight="426.0" prefWidth="641.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="fxml.MainController">
	<children>
		<TextField id="search_txt" fx:id="search_text"
			layoutX="160.0" layoutY="183.0" prefHeight="32.0" prefWidth="349.0"
			promptText="search"
			style="-fx-border-radius: 0; -fx-border-style: SOLID; -fx-border-width: 2; -fx-effect: BLOOM; -fx-border-color: #00FF7F;" />
		<Button fx:id="login_btn" layoutX="575.0" layoutY="14.0"
			mnemonicParsing="false" onAction="#LoginClick"
			style="-fx-background-color: #00FF7F;" text="login" />
		<Button fx:id="register_btn" layoutX="509.0" layoutY="14.0"
			mnemonicParsing="false" onAction="#RegisterClick"
			style="-fx-background-color: #00FF7F;" text="register" />
		<RadioButton layoutX="172.0" layoutY="215.0"
			mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0"
			text="name" textFill="#383938" />
		<RadioButton layoutX="279.0" layoutY="215.0"
			mnemonicParsing="false" prefHeight="30.0" prefWidth="112.0"
			text="description" textFill="#383938" />
		<RadioButton layoutX="404.0" layoutY="215.0"
			mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0"
			text="place" textFill="#383938" />
		<AnchorPane layoutY="-1.0" prefHeight="433.0"
			prefWidth="56.0" style="-fx-background-color: grey;">
			<effect>
				<Bloom />
			</effect>
		</AnchorPane>
	</children>
</AnchorPane>
